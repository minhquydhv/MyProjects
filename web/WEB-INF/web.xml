        <servlet-name>EvaluateTeamLeader</servlet-name>
        <url-pattern>/kpi/evaluateTeamLeader</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EvaluateHoD</servlet-name>
        <url-pattern>/kpi/evaluateHoD</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
