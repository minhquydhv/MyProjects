    </servlet>
    <servlet>
        <servlet-name>ConfirmedKPIGD</servlet-name>
        <servlet-class>cotroller.confirmed.individual.ConfirmedKPIGD</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EvaluateStaff</servlet-name>
        <servlet-class>cotroller.evaluate.individual.EvaluateStaff</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EvaluateTeamLeader</servlet-name>
        <servlet-class>cotroller.evaluate.individual.EvaluateTeamLeader</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EvaluateHoD</servlet-name>
        <servlet-class>cotroller.evaluate.individual.EvaluateHoD</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CheckLogin</servlet-name>
        <url-pattern>/kpi/checklogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/kpi/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProcessSettingIndividual</servlet-name>
        <url-pattern>/kpi/process</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Test</servlet-name>
        <url-pattern>/Test</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProcessSendMail</servlet-name>
        <url-pattern>/kpi/processSendMail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConfirmedKPITeamLeader</servlet-name>
        <url-pattern>/kpi/confirmedKPITeamLeader</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConfirmedKPIIndividual</servlet-name>
        <url-pattern>/kpi/confirmedKPIHoD</url-pattern>
